{
    "extends": "./node_modules/eslint-config-ovh/configs/es5-browser.js",
    "rules": {
        "one-var": "warn",
        "no-unused-vars": "warn",
        "no-void": "warn",
        "no-cond-assign": "warn",
        "no-mixed-operators" : "warn",
        "newline-per-chained-call": "warn",
